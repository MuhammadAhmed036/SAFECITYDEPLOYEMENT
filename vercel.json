{
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "DB_HOST": "",
    "DB_PORT": "",
    "DB_NAME": "",
    "DB_USER": "",
    "DB_PASSWORD": "",
    "NEXT_PUBLIC_API_BASE": ""
  },
  "build": {
    "env": {
      "DB_HOST": "",
      "DB_PORT": "",
      "DB_NAME": "",
      "DB_USER": "",
      "DB_PASSWORD": "",
      "NEXT_PUBLIC_API_BASE": ""
    }
  },
  "rewrites": [
    {
      "source": "/api/proxy/(.*)",
      "destination": "/api/proxy/$1"
    }
  ]
}