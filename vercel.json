{
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "DB_HOST": "@db-host",
    "DB_PORT": "@db-port",
    "DB_NAME": "@db-name",
    "DB_USER": "@db-user",
    "DB_PASSWORD": "@db-password",
    "NEXT_PUBLIC_API_BASE": "@next-public-api-base"
  },
  "build": {
    "env": {
      "DB_HOST": "@db-host",
      "DB_PORT": "@db-port",
      "DB_NAME": "@db-name",
      "DB_USER": "@db-user",
      "DB_PASSWORD": "@db-password",
      "NEXT_PUBLIC_API_BASE": "@next-public-api-base"
    }
  },
  "rewrites": [
    {
      "source": "/api/proxy/(.*)",
      "destination": "/api/proxy/$1"
    }
  ]
}